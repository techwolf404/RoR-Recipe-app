
<h1 class="heading">Recipe foods</h1>

<div id="recipe_foods">
  <% @recipe_foods.each do |recipe_food| %>
    <%= render recipe_food %>
    <p>
      <%= link_to "Show this recipe food", recipe_food %>
    </p>
  <% end %>
</div>

<button class="btn btn-secondary newbtn"><%= link_to "New recipe food", new_recipe_food_path %></button>
